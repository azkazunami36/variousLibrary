{
    "compilerOptions": {
        "module": "ESNext",
        "moduleResolution": "Node",
        "target": "ESNext",
        "resolveJsonModule": true,
        "composite": true,
        "declaration": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "noImplicitAny": true,
        "sourceMap": true,
        "strictNullChecks": true
    },
    "include": [
        "sumLibrary.ts",
        "external_modules",
        "orignal_modules"
    ],
    "exclude": [
        "node_modules"
    ]
}
