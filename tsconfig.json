{
    "compilerOptions": {
        "module": "CommonJS",
        "moduleResolution": "Node",
        "target": "ESNext",
        "resolveJsonModule": true,
        "declaration": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "noImplicitAny": true,
        "strictNullChecks": true,
        "strict": true,
        "newLine": "lf",
        "outDir": "dist"
    },
    "include": ["sumLibrary.ts", "external_modules", "orignal_modules"],
    "exclude": ["node_modules"]
}
